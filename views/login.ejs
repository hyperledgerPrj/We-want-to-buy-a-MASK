    <link rel="stylesheet" href="css/style.css" />
    <style>
      body {text-align: center;}
      input {height: 40px;}
      a{text-decoration: none;}
      table{height: 100px;
      margin: auto;
      text-align: center;}

       input[type="submit"] {

                 background: #09c;
                 color: #fff;
                 font-family: 'Sunflower', sans-serif;
                 height: 40px;
                 weight: 100px;
                 border: 1px solid #999;
}

 </style>


  </head>
  <!-- End Head -->

  <body class="default">
    <!--
START MODULE AREA 1: Header 3
-->
<header class="MOD_HEADER3">
  <div class="MOD_HEADER3_SocialBarContainer">
      <div data-layout="_r" class="MOD_HEADER3_SocialBar">
          <div class="MOD_HEADER3_SocialIcons">

              <div class="menubar">
                  <nav>
                      <ul>
                          <li>
                              <a href="/" class="text">K-SHIELD</a>
                          </li>
                          <li><a href="#" class="view" onclick="myFunction()">View Mask</a></li>
                          <li>
                          <li><a href="#" class="view" onclick="myFunction()">View Agents</a></li>

                          <li>
                          <li><a href="#" class="view" onclick="myFunction()">Agents register</a></li>


                      </ul>
                  </nav>
              </div>
          </div>
          <div class="MOD_HEADER3_Contact">
              <p class="MOD_HEADER3_Tel"></p>
              <a href="/signup" class="btn"> Sign up </a>
              <a href="/login" class="btn"> Login </a>
          </div>
      </div>
  </div>
</header>
    <!--
  START MODULE AREA 2: Page Title 1
--><main>
<section>
  <form name="loginform" action="/login" method="post">
  <div data-layout="_r">
    <div data-layout="al16"><br>
      <h1 style="text-align: center;">Login</h1><br>
    </div>
    <table class="loginform">
      <tr>
        <td>
          <label for="labelid"
          >아이디 <span class="required"></span></label
        >
          <input type="text" name="userid"  placeholder="ID"
          required="required"
          aria-required="true" />
        </td>
      </tr>
      <tr>
        <td>
          <label for="passwordlabel"
          >비밀번호 <span class="required"></span></label
        >
          <input type="password" name="userpw" placeholder="Password"
          required="required"
          aria-required="true" />
        </td>
      </tr>
      <tr>
        <td>
        </td>
      </tr>
      <tr>
        <tr>
          <td>
          </td>
        </tr>
        <br>
        <br>
        <tr>
        <td>
          <input type="submit" name="submit" value="Login"
          required="required"
          aria-required="true" onclick="login();"/>
        </td>
      </tr>
    </table>
  </div>
</form>
</section>
</main>
<!--
END MODULE AREA 3: Promo Box 1
-->
<script>
function myFunction() {
alert("로그인 하세요");
}
function login() {
alert("로그인 되었습니다!");
location.href = "indexlogin.html";

}


</script>
<script src="/js/index.js"></script>

</body>
</html>
